@article{hunter2021,
  title = {Relationship {{Between Tasked Vocal Effort Levels}} and {{Measures}} of {{Vocal Intensity}}},
  author = {Hunter, Eric J. and Berardi, Mark L. and van Mersbergen, Miriam},
  options = {useprefix=true},
