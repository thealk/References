@article{titze2020,
  title = {Vocal {{Loudness Variation With Spectral Slope}}},
  author = {Titze, Ingo R. and Palaparthi, Anil},
  date = {2020-01-22},
  journaltitle = {Journal of Speech, Language, and Hearing Research},
  shortjournal = {J Speech Lang Hear Res},
  volume = {63},
