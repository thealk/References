@article{mendel_speech_2008,
  title = {Speech Understanding Using Surgical Masks: A Problem in Health Care?},
  author = {Mendel, Lisa Lucks and Gardino, Julie A. and Atcherson, Samuel R.},
  date = {2008},
  journaltitle = {Journal of the American Academy of Audiology},
  volume = {19},
  number = {9},
  pages = {686--695},
  issn = {1050-0545},
  doi = {10.3766/jaaa.19.9.4},
  abstract = {BACKGROUND: Successful communication is necessary in health-care environments. Yet the presence of noise in hospitals, operating rooms, and dental offices may have a deleterious effect on health-care personnel and patients understanding messages accurately. The presence of a surgical mask and hearing loss may further affect speech perception. PURPOSE: To evaluate whether a surgical mask had an effect on speech understanding for listeners with normal hearing and hearing impairment when speech stimuli were administered in the presence or absence of dental office noise. RESEARCH DESIGN: Participants were assigned to one of two groups based on hearing sensitivity in this quasi-experimental, cross-sectional study. STUDY SAMPLE: A total of 31 adults participated in this study (1 talker, 15 listeners with normal hearing, and 15 with hearing impairment). The normal hearing group had thresholds of 25 dB HL or better at the octave frequencies from 250 through 8000 Hz while the hearing loss group had varying degrees and configurations of hearing loss with thresholds equal to or poorer than 25 dB HL for the same octave frequencies. DATA COLLECTION AND ANALYSIS: Selected lists from the Connected Speech Test (CST) were digitally recorded with and without a surgical mask present and then presented to the listeners in four conditions: without a mask in quiet, without a mask in noise, with a mask in quiet, and with a mask in noise. RESULTS: A significant difference was found in the spectral analyses of the speech stimuli with and without the mask. The presence of a surgical mask, however, did not have a detrimental effect on speech understanding in either the normal-hearing or hearing-impaired groups. The dental office noise did have a significant effect on speech understanding for both groups. CONCLUSIONS: These findings suggest that the presence of a surgical mask did not negatively affect speech understanding. However, the presence of noise did have a deleterious effect on speech perception and warrants further attention in health-care environments.},
  langid = {english},
  keywords = {*Communication,*Dentist-Patient Relations,*Masks,adult,Adult,aged,Aged,article,Audiometry,Auditory Perception,auditory stimulation,auditory threshold,background noise,Communication,controlled study,Dental Offices,Dentist-Patient Relations,Female,health care,hearing acuity,Hearing Disorders,hearing impairment,Hearing Loss,Hearing Loss/complications,human,Humans,listeners,major clinical study,male,Male,Masks,Middle Aged,noise,Noise,Noise Effects,Perceptual Masking,priority journal,Pure-Tone,Speech,speech discrimination,Speech Intelligibility,speech perception,Speech Perception,stimulus response,surgical mask,surgical masks,Tooth Diseases,Tooth Diseases/complications,Young Adult}
}

