@book{hamlaoui2015,
  title = {Acoustic {{Correlates}} of {{Focus Marking}} in {{Polish}}},
  author = {Hamlaoui, Fatima and Zygis, Marzena and Engelmann, Jonas and Wagner, Michael},
  date = {2015-08-10},
  abstract = {Languages vary in the types of contexts that affect prosodic prominence. This paper reports on a production study investigating how different types of foci influence prosody in Polish. The results show that focus and givenness in Polish are both marked prosodically, with pitch and intensity as the main acoustic correlates. Polish patterns like En-glish in showing prosodic focus marking in a broad range of contexts, and differs in this regard from Romance languages, despite the fact that aspects of the prosodic system of Polish are more similar to Romance. Finally, the results do not support the claim in [7, 8] that word prominence is shifted from the penult syllable to the initial syllable under focus.}
}

