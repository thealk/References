@incollection{clopper2018,
  title = {2. {{Exploring}} Variation in Phonetic Reduction: {{Linguistic}}, Social, and Cognitive Factors},
  shorttitle = {2. {{Exploring}} Variation in Phonetic Reduction},
  booktitle = {Rethinking {{Reduction}}},
  editor = {Cangemi, Francesco and Clayards, Meghan and Niebuhr, Oliver and Schuppler, Barbara and Zellers, Margaret},
  date = {2018-06-25},
