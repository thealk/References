@article{muzzi_short_2021,
  title = {Short Report on the Effects of {{SARS-CoV-2}} Face Protective Equipment on Verbal Communication.},
  author = {Muzzi, Enrico and Chermaz, Carol and Castro, Veronica and Zaninoni, Mattia and Saksida, Amanda and Orzan, Eva},
  date = {2021},
