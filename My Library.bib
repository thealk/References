@article{wood1992,
  title = {Reliability of Labial Closure Force Measurements in Normal Subjects and Patients with {{CNS}} Disorders.},
  author = {Wood, L. Maureen and Hughes, Julie and Hayes, Keith C. and Wolfe, Dalton L.},
  date = {1992-04-01},
  journaltitle = {Journal of Speech, Language, and Hearing Research},
  volume = {35},
