@article{tripoliti2011a,
  title = {Treatment of Dysarthria Following Subthalamic Nucleus Deep Brain Stimulation for {{Parkinson}}'s Disease},
  author = {Tripoliti, Elina and Strong, Laura and Hickey, Freya and Foltynie, Tom and Zrinzo, Ludvic and Candelario, Joseph and Hariz, Marwan and Limousin, Patricia},
  date = {2011},
  journaltitle = {Movement Disorders},
  volume = {26},
  number = {13},
  pages = {2434--2436},
  issn = {1531-8257},
  doi = {10.1002/mds.23887},
  url = {https://movementdisorders.onlinelibrary.wiley.com/doi/abs/10.1002/mds.23887},
  urldate = {2021-05-25},
  abstract = {Background: Deep brain stimulation of the subthalamic nucleus (STN-DBS) is an established treatment for patients with Parkinson's disease (PD). Speech impairment is a frequent side effect of the surgery. This study examined the efficacy of an intensive speech treatment, the Lee Silverman Voice Treatment (LSVT) on dysarthria after STN-DBS. Methods: The LSVT was administered to 10 patients with STN-DBS (surgical group) and 10 patients without (medical group). Patients were assessed before, immediately after, and 6 months following the speech treatment using sustained phonation, a speech intelligibility scale, and monologue. Vocal loudness, speech intelligibility, and perceptual ratings were the primary outcome measures. Results: Vocal loudness and perceptual scores improved significantly across tasks for the medical group only. Speech intelligibility did not significantly change for either group. Results in the surgical group were variable, with some patients deteriorating. Conclusions: Treatment of dysarthria following STN-DBS needs further investigation because of the variable response to LSVT. © 2011 Movement Disorder Society},
  langid = {english},
  keywords = {LSVT,Parkinson's disease,speech,STN-DBS},
  annotation = {\_eprint: https://movementdisorders.onlinelibrary.wiley.com/doi/pdf/10.1002/mds.23887},
  file = {/Users/theaknowles/Zotero/storage/T4GESTYZ/Tripoliti et al. - 2011 - Treatment of dysarthria following subthalamic nucl.pdf;/Users/theaknowles/Zotero/storage/IXSLZYY6/mds.html}
}

