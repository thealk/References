@article{lane2007,
  title = {Effects of {{Short-}} and {{Long-Term Changes}} in {{Auditory Feedback}} on {{Vowel}} and {{Sibilant Contrasts}}},
  author = {Lane, Harlan and Matthies, Melanie L. and Guenther, Frank H. and Denny, Margaret and Perkell, Joseph S. and Stockmann, Ellen and Tiede, Mark and Vick, Jennell and Zandipour, Majid},
  date = {2007-08},
  journaltitle = {Journal of Speech, Language, and Hearing Research},
