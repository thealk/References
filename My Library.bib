  author = {Vasquez-Correa, Juan Camilo and Orozco-Arroyave, Juan Rafael and Arora, Raman and Nöth, Elmar and Dehak, Najim and Christensen, Heidi and Rudzicz, Frank and Bocklet, Tobias and Cernak, Milos and Chinaei, Hamidreza and others},
