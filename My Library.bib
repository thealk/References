@article{mauritzson-sandberg_evaluation_1995,
  title = {An Evaluation of Respiratory Protective Devices Used in Children's Evacuation.},
  author = {Mauritzson-Sandberg, E.},
  date = {1995},
  journaltitle = {Ergonomics},
  volume = {38},
