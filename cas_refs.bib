@article{olmsteadRoleSpeakerListener2020,
  title = {The {{Role}} of the {{Speaker}}, the {{Listener}}, and {{Their Joint Contributions During Communicative Interactions}}: {{A Tripartite View}} of {{Intelligibility}} in {{Individuals With Dysarthria}}},
  shorttitle = {The {{Role}} of the {{Speaker}}, the {{Listener}}, and {{Their Joint Contributions During Communicative Interactions}}},
  author = {Olmstead, Annie J. and Lee, Jimin and Viswanathan, Navin},
  date = {2020-04-27},
  journaltitle = {Journal of Speech, Language, and Hearing Research},
  shortjournal = {J Speech Lang Hear Res},
  volume = {63},
  pages = {1106--1114},
  issn = {1092-4388, 1558-9102},
  doi = {10.1044/2020_JSLHR-19-00233},
  url = {http://pubs.asha.org/doi/10.1044/2020_JSLHR-19-00233},
  urldate = {2021-03-25},
  abstract = {Purpose               We present a tripartite view of intelligibility in which the contributions of both the speaker and listener, as well as their joint effort during interaction, are considered. While considerable research has examined communicative interactions in situ, there is a critical gap in current knowledge on how speech intelligibility unfolds during such interactions. Here, we argue that research examining speech intelligibility in communicative interactions may provide important groundwork for advancement in clinical interventions for individuals with dysarthria.                                         Method               First, we describe the view and argue for its consideration as a powerful way of thinking about speech intelligibility. We then briefly situate the view in the relevant literature on speech intelligibility and existing theoretical frameworks. We then identify suitable methodological paradigms for studying joint contributions to intelligibility and, lastly, discuss the clinical application and potential impact of this tripartite view.                                         Conclusions               Speech communication occurs through interaction; however, in the laboratory and clinic, emphasis is usually placed on individual speakers and listeners. We have proposed that it is critical to consider how the joint contributions of speakers and listeners affect speech intelligibility in communicative interaction. This conceptualization is well aligned with the International Classification of Functioning, Disability and Health, and the findings from such an approach will allow us to better understand how to maximize available resources to enhance speech intelligibility.                        ,},
  file = {/Users/theaknowles/Zotero/storage/7J38KA7M/Olmstead et al. - 2020 - The Role of the Speaker, the Listener, and Their J.pdf},
  langid = {english},
  number = {4}
}

